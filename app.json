{
  "expo": {
    "name": "RN-EXPO",
    "slug": "postku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contains",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Poppins-Regular.ttf",
            "./assets/Poppins-Medium.ttf",
            "./assets/Poppins-SemiBold.ttf",
            "./assets/Poppins-Thin.ttf",
            "./assets/Poppins-Light.ttf",
            "./assets/Poppins-Bold.ttf"
          ]
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8e81756e-2489-49e9-af30-95a1ddfb1eb8"
      }
    },
    "owner": "dfaw17"
  }
}
